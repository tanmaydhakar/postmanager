{
  "name": "postmanager",
  "version": "1.0.0",
  "description": "Social media post manager",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon",
    "test": "mocha ./test/testCases.js --timeout 30000 --exit",
    "cover": "NODE_ENV=development nyc --reporter=lcov --reporter=html --reporter=text npm run test"
  },
  "nyc": {
    "include-all-sources": true,
    "register": "babel-register",
    "all": true,
    "exclude": [
      "test",
      "seeders",
      "index.js",
      "init.js",
      "models.js",
      "routes.js",
      "coverage",
      "migrations",
      "modules/events",
      "modules/roles",
      "modules/scheduledPosts",
      "modules/tokens",
      "modules/userRoles",
      "modules/*/*.routes.js",
      "modules/*/*.routes.js",
      "utilities/events.js",
      "utilities/fileUpload.js",
      "utilities/schedulePost.js"
    ]
  },
  "author": "tanmay",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.794.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "fs": "0.0.1-security",
    "googleapis": "^64.0.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.15",
    "nodemon": "^2.0.6",
    "path": "^0.12.7",
    "pg": "^8.4.2",
    "save": "^2.4.0",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sequelize-cli": "^6.2.0"
  }
}
