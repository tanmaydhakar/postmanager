{
  "env": {
    "browser": true,
    "es2020": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "no-undef": "off",
    "import/no-dynamic-require": "off",
    "func-names": "off",
    "no-param-reassign": "off",
    "no-await-in-loop": "off",
    "no-underscore-dangle": "off",
    "global-require": "off"
  }
}
